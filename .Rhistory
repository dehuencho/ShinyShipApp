pi
p1 <- c(54.77127, 18.99692)
p2 <- c(54.76542, 18.99361)
R = 6371E3
phi1 = p1[1]*pi/180
phi2 = p2[1]*pi/180
delta_phi = (p2[1]-p1[1])*pi/180
delta_lambda = (p2[2]-p1[2])*pi/180
a = sin(delta_phi/2) * sin(delta_phi/2) +
cos(phi1) * cos(phi2)*
sin(delta_lambda/2) * sin(delta_lambda/2)
c = 2 * atan2(sqrt(a), sqrt(1-a))
d = R*c
(d = R*c)
